#include <iostream>
#include <vector>
#include <string>
using namespace std;

int main() {
    string S = "ababcabcabababdabd";
    string P = "aba";

    vector<int> positions;
    int n = S.size();
    int m = P.size();

    // Recherche na√Øve
    for (int i = 0; i <= n - m; i++) {
        bool ok = true;

        for (int j = 0; j < m; j++) {
            if (S[i + j] != P[j]) {
                ok = false;
                break;
            }
        }

        if (ok)
            positions.push_back(i);
    }

    // Affichage
    cout << "Positions : ";
    for (int pos : positions)
        cout << pos << " ";
    cout << endl;

    return 0;
}