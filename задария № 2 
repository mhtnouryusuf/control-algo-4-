#include <iostream> 
include < sstream> 
include < string> 

unsing namespace std; 

int main ()
 
{ 
    string input = "I LOVE programming "; 
    stringstream ss(input); 
    string word , longest = "";

    while ( ss >> word ) {
        if ( word .length () > longest.length()) { 

                   longest= word; 


        }
    } 
    cout << longest << endl; 

    return 0 ; 
}