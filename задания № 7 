#include <iostream>
using namespace std;

// Somme des chiffres r√©cursive
int sommeChiffres(int n) {
    if (n == 0)
        return 0;
    return (n % 10) + sommeChiffres(n / 10);
}

int main() {
    int n = 12345;

    cout << "Somme des chiffres de " << n << " = " << sommeChiffres(n) << endl;

    return 0;
}